'''
'''
command = ""
started = False

while True:

    command = input("> ").lower()

    if command == "start":
        if started:
            print("car is started")

        else:
            started = True
            
            print("car is started...") 
 
 
    elif command == "stop":
        if not started:
            print("car is already stopped")
            
            print("car is stopped")

    elif command == "help":
        print(

            start - to start the car.
            stop - to stop the car.
            quit - to quit.

        )
    

     

